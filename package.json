{
    "name": "react-meny-app",
    "version": "0.9.9",
    "description": "Mobile app and web components for chain-sites",
    "main": "index.js",
    "engines": {
        "node": "8.4.0"
    },
    "scripts": {
        "test": " jest",
        "run": "npm run start",
        "start": "node server.js",
        "build": "gulp build && webpack --config webpack.mobile.js --env.NODE_ENV=production",
        "build:travis": "npm run build && npm run build:chainsite",
        "build-native": "webpack  --env.DEST=native --watch  --config webpack.mobile.js & DEST=native gulp",
        "build:mobile": "gulp build & webpack --env.NODE_ENV=production --config webpack.mobile.js",
        "build:chainsite": "webpack --config webpack.chainsite.js --env.NODE_ENV=production",
        "size:chainsite": "webpack --config webpack.chainsite.js --env.NODE_ENV=production --json | webpack-bundle-size-analyzer",
        "watch:mobile": "webpack --watch --config webpack.mobile.js & gulp",
        "watch:chainsite": "webpack --config webpack.chainsite.js --watch & gulp chainsite:watch",
        "run:mobile": "npm run watch:mobile",
        "run:chainsite": "npm run watch:chainsite & npm start",
        "lint": "eslint src/**/*",
        "lint:debug": "stylelint resources/sass/**/*.scss; eslint src/**/*; exit 0",
        "ngrok": "ngrok http localhost:3000",
        "preinstall": "npm config set package-lock false"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/MWNG/react-meny-app.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/MWNG/react-meny-app/issues"
    },
    "homepage": "https://github.com/MWNG/react-meny-app#readme",
